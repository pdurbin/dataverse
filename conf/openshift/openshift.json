{
  "kind": "Template",
  "apiVersion": "v1",
  "metadata": {
    "name": "dataverse",
    "labels": {
      "name": "dataverse"
    },
    "annotations": {
      "openshift.io/description": "Dataverse is open source research data repository software: https://dataverse.org",
      "openshift.io/display-name": "Dataverse"
    }
  },
  "objects": [
    {
      "kind": "Service",
      "apiVersion": "v1",
      "metadata": {
        "name": "dataverse-glassfish-service"
      },
      "spec": {
        "ports": [
          {
            "name": "web",
            "protocol": "TCP",
            "port": 8080,
            "targetPort": 8080
          }
        ]
      }
    },
    {
      "kind": "ImageStream",
      "apiVersion": "v1",
      "metadata": {
        "name": "ndslabs-dataverse"
      },
      "spec": {
        "dockerImageRepository": "ndslabs/dataverse"
      }
    },
    {
      "kind": "DeploymentConfig",
      "apiVersion": "v1",
      "metadata": {
        "name": "frontend",
        "annotations": {
          "template.alpha.openshift.io/wait-for-ready": "true"
        }
      },
      "spec": {
        "containers": [
          {
            "name": "ndslabs-dataverse",
            "image": "ndslabs-dataverse",
            "ports": [
              {
                "containerPort": 8080,
                "protocol": "TCP"
              }
            ]
          }
        ]
      }
    }
  ]
}
